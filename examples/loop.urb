include(urb_dictionary.h)

// we need this to prevent urb from interpreting our variables
goto(start)

local:
    i: 0
start:
    println($i)
    set(mem, i, add($i, 1))
    // can look tricky but is actually very simple to understand
    // it basically says:
    // if $i is lesser or equals to 100000 goto start
    // or
    // if (i <= 100000) goto start;
    jif(le($i, 100000), start)