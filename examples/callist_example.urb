#include "../build/dictionary.h"
#include "../lib/syntax.h"

// create a procedure and
// push it to the context
push(CONTEXT, call(pack, 4, 123, print, bypass(CONTEXT), bypass(CALL)))

// call the procedure
callist(get(CONTEXT, -1))

// call println
call(println)

free(get(CONTEXT, -1))

