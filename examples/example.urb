#include "../etc/syntax.h"

// we need this to prevent urb from interpreting our local scope
goto(start)

// there is no reason to pollute our global scope(context)
// lets store our local variables here in the code
local:
    a: 50
    b: 123
    str: \abcde1\s234\n
start:
    call(println, $b)
    
    call(println, $a)
    
    call(println, start)

    call(println, start)

    call(pring, mem, str)
    
call(ls, mem)
