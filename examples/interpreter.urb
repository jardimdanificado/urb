include(urb_dictionary.h)

// standalone interpreter alternative to embedding urb
goto(start)
local:
    file: 0
    error_message: \you\sneed\sto\spass\sa\sfilename!\n
start:
    jif(lt(len(args), 2), error)
    set(mem, file, load_urbin(get(args, 1)))
    interpret(exec, $file, stack)
    goto(free_before_end)
error:
    pring(pointer(mem, error_message))
    goto(end)
free_before_end:
    free($file)
end: