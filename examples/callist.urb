include(urb_dictionary.h)

define(callist, {{interpret(exec, $1, stack)}})

// we need this to prevent urb from interpreting our variables
goto(start)

local:
    i: 0
    str: \hello\sworld!\n
    list: 0
test_proc:
    pring(pointer(mem, sub(str, local)))
start:
    set(mem, list, sublist(mem, local, sub(start, local)))
    callist($list)
end:
    free($list)